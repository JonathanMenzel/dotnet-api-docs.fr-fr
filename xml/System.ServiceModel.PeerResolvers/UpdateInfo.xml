        <param name="registrationId">
          <see cref="T:System.Guid" /> qui spécifie l'ID unique de l'entrée d'inscription à mettre à jour.</param>
        <param name="client">
          <see cref="T:System.Guid" /> qui spécifie l'ID unique du client homologue qui a envoyé les informations d'entrée d'inscription mises à jour.</param>
