      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.WindowsClientElement">
          <source>Represents a configuration element that specifies the settings for a Windows credential to be used to represent the client.</source>
          <target state="translated">Représente un élément de configuration qui spécifie les paramètres pour une information d'identification Windows à utiliser pour représenter le client.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.WindowsClientElement">
          <source>This class cannot be inherited.</source>
          <target state="translated">Cette classe ne peut pas être héritée.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsClientElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.WindowsClientElement" /&gt;</ph> class.</source>
          <target state="translated">Initialise une nouvelle instance de la classe <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.WindowsClientElement" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowedImpersonationLevel">
          <source>Gets or sets the permissible impersonation level.</source>
          <target state="translated">Obtient ou définit le niveau d'emprunt d'identité autorisé.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowedImpersonationLevel">
          <source>A valid <ph id="ph1">&lt;see cref="T:System.Security.Principal.TokenImpersonationLevel" /&gt;</ph> value that specifies the permissible impersonation level.</source>
          <target state="translated">Valeur <ph id="ph1">&lt;see cref="T:System.Security.Principal.TokenImpersonationLevel" /&gt;</ph> valide qui spécifie le niveau d'emprunt d'identité autorisé.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowedImpersonationLevel">
          <source>The default is <ph id="ph1">&lt;see cref="F:System.Security.Principal.TokenImpersonationLevel.Identification" /&gt;</ph>.</source>
          <target state="translated">La valeur par défaut est <ph id="ph1">&lt;see cref="F:System.Security.Principal.TokenImpersonationLevel.Identification" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowedImpersonationLevel">
          <source>See <ph id="ph1">&lt;xref:System.Security.Principal.TokenImpersonationLevel&gt;</ph> for a description of the enumeration.</source>
          <target state="translated">Pour une description de l'énumération, consultez <ph id="ph1">&lt;xref:System.Security.Principal.TokenImpersonationLevel&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowNtlm">
          <source>Gets or sets a boolean value that specifies whether NTLM authentication is allowed.</source>
          <target state="translated">Obtient ou définit une valeur booléenne qui spécifie si l'authentification NTLM est autorisée.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowNtlm">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if Windows security is enabled in the workgroup scenario; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> si la sécurité Windows est activée dans le scénario de groupe de travail ; sinon, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowNtlm">
          <source>The default is <ph id="ph1">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">La valeur par défaut est <ph id="ph1">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowNtlm">
          <source>When this property is set to <ph id="ph1">`true`</ph>, Windows security is enabled at the workgroup level.</source>
          <target state="translated">Lorsque cette propriété a la valeur <ph id="ph1">`true`</ph>, la sécurité Windows est activée au niveau du groupe de travail.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsClientElement.AllowNtlm">
          <source>Otherwise, Windows security is only supported at the domain level.</source>
          <target state="translated">Sinon, la sécurité Windows est prise en charge uniquement au niveau du domaine.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsClientElement.Copy(System.ServiceModel.Configuration.WindowsClientElement)">
          <source>The configuration element to be copied.</source>
          <target state="translated">L'élément de configuration à copier.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsClientElement.Copy(System.ServiceModel.Configuration.WindowsClientElement)">
          <source>Copies the content of the specified configuration element to this configuration element.</source>
          <target state="translated">Copie le contenu de l'élément de configuration spécifié vers cet élément de configuration.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsClientElement.Copy(System.ServiceModel.Configuration.WindowsClientElement)">
          <source><ph id="ph1">&lt;paramref name="from" /&gt;</ph> is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="from" /&gt;</ph> est <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsClientElement.Copy(System.ServiceModel.Configuration.WindowsClientElement)">
          <source>The configuration file is read-only.</source>
          <target state="translated">Le fichier de configuration est en lecture seule.</target>       </trans-unit>
