      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>Enables you to set contract and tool options at assembly, type, or method granularity.</source>
          <target state="translated">Permet de définir un contrat et des options d'outil pour la granularité d'assembly, de type ou de méthode.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>The following table shows the currently supported options.</source>
          <target state="translated">Le tableau suivant montre les options actuellement pris en charge.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>Category</source>
          <target state="translated">Category</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>Setting</source>
          <target state="translated">Paramètre</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>Value/effect</source>
          <target state="translated">Valeur/effet</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>contract</source>
          <target state="translated">contrat</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>inheritance</source>
          <target state="translated">héritage</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source><ph id="ph1">`true`</ph> to turn contract inheritance on; <ph id="ph2">`false`</ph> to turn it off.</source>
          <target state="translated"><ph id="ph1">`true`</ph> Pour activer l’héritage de contrat ; <ph id="ph2">`false`</ph> pour le désactiver.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>The default is <ph id="ph1">`true`</ph>.</source>
          <target state="translated">La valeur par défaut est <ph id="ph1">`true`</ph>.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>runtime</source>
          <target state="translated">runtime</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>checking</source>
          <target state="translated">vérifier</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source><ph id="ph1">`true`</ph> to turn run-time checking on; <ph id="ph2">`false`</ph> to turn it off.</source>
          <target state="translated"><ph id="ph1">`true`</ph> Pour activer le contrôle à l’exécution. <ph id="ph2">`false`</ph> pour le désactiver.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>The default is <ph id="ph1">`true`</ph>.</source>
          <target state="translated">La valeur par défaut est <ph id="ph1">`true`</ph>.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>You can use this attribute as illustrated in the following examples.</source>
          <target state="translated">Vous pouvez utiliser cet attribut comme illustré dans les exemples suivants.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>To turn off run-time checking for the entire assembly:</source>
          <target state="translated">Pour désactiver la vérification de la totalité de l’assembly d’exécution :</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>To turn run-time contract checking on for a specific type:</source>
          <target state="translated">Pour activer la vérification du contrat d’exécution pour un type spécifique :</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
          <source>To turn run-time checking off for a specific method:</source>
          <target state="translated">Pour désactiver contrôle à l’exécution d’une méthode spécifique :</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="T:System.Diagnostics.Contracts.ContractOptionAttribute">
